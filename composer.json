{
    "version": "1.0",
    "licence": "MIT",
    "name": "mhbarry/resourcefy",
    "description": "Easy app builder",
    "license": "license",
    "authors": [
        {
            "name": "Mamadou Hady Barry",
            "email": "seidya.dev.mhb@gmail.com",
            "homepage": "https://github.com/mhbarry/resourcefy"
        }
    ],
    "homepage": "https://github.com/laranuxt/resourcefy",
    "keywords": ["framework", "Laravel", "Resourcefy"],
    "require": {
        "illuminate/support": "~5|~6|~7|~8",
        "laravel/passport": "^10.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~8.0|~9.0",
        "mockery/mockery": "^1.1",
        "orchestra/testbench": "~3|~4|~5|~6",
        "sempro/phpunit-pretty-print": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Laranuxt\\Resourcefy\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Laranuxt\\Resourcefy\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Mhbarry\\Resourcefy\\ResourcefyServiceProvider"
            ],
            "aliases": {
                "Resourcefy": "Mhbarry\\Resourcefy\\Facades\\Resourcefy"
            }
        }
    }
}
